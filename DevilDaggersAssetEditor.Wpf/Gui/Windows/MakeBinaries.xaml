<Window x:Class="DevilDaggersAssetEditor.Wpf.Gui.Windows.MakeBinariesWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d"
        Title="Make binaries" Height="768" Width="768" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
	<StackPanel Name="Main">
		<StackPanel Margin="6" Background="{StaticResource Gray3}">
			<StackPanel Style="{StaticResource PaddingStackPanel}">
				<Label>Mod name</Label>
				<TextBox Name="TextBoxModName" TextChanged="TextBoxModName_TextChanged" />
			</StackPanel>
		</StackPanel>
		<StackPanel Margin="6" Background="{StaticResource Gray3}">
			<StackPanel Style="{StaticResource PaddingStackPanel}">
				<Button Name="ButtonMakeBinaries" Content="Make binaries" Click="MakeBinaries_Click" Style="{StaticResource OkButtonLarge}" />
			</StackPanel>
		</StackPanel>
	</StackPanel>
</Window>
