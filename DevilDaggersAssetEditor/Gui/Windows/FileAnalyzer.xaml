<Window x:Class="DevilDaggersAssetEditor.Gui.Windows.FileAnalyzerWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d"
        Title="File analyzer" Height="512" Width="1366" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
	<StackPanel>
		<Button Name="ButtonBrowse" Click="ButtonBrowse_Click" Width="128">Browse binary file</Button>
		<Canvas Name="C" Width="1280" Height="32" />
		<Label Name="FileSize" HorizontalAlignment="Center" />
		<Grid Name="Data" />
	</StackPanel>
</Window>